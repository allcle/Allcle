                            <ColumnDefinition />
                            <ColumnDefinition Width="10"/>
                            <ColumnDefinition Width="3*"/>
                        </Grid.ColumnDefinitions>
                        <ComboBox x:Name="LunchFrom" Grid.Column="0" SelectedIndex="0">
                            <ComboBoxItem>선택안함</ComboBoxItem>
