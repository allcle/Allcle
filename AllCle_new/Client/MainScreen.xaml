                            <ComboBoxItem>영어계열</ComboBoxItem>
                            <ComboBoxItem>제2외국어계열</ComboBoxItem>
                            <ComboBoxItem>교직과목</ComboBoxItem>
                        </ComboBox>

                        <ComboBox x:Name="engineer_ABEEK" Grid.Column="6" FontSize="10" Margin="0" SelectedIndex="0" Visibility="Collapsed" DropDownClosed="engineer_ABEEK_DropDownClosed">
                            <ComboBoxItem>전체</ComboBoxItem>
                            <ComboBoxItem>기초교양</ComboBoxItem>
                            <ComboBoxItem>일반교양</ComboBoxItem>
